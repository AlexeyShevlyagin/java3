package Practic3.Obolochka;
//import java.lang.*;

public class Double_obolochka {
    public static void main(String[] args) {
        int a = 25;
        double b = 12.12;
        String c = "123432";
        double x = Double.valueOf(a);
        System.out.println(x);
        x = Double.valueOf(b);
        System.out.println(x);
        x = Double.valueOf(c);
        System.out.println(x);
    }
}
